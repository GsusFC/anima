# AnimaGen Production Configuration
NODE_ENV=production
PORT=3001

# File handling
OUTPUT_DIR=output
TEMP_DIR=uploads
MAX_FILE_SIZE=52428800
MAX_FILES=50

# Video processing
DEFAULT_FPS=30
PREVIEW_MAX_WIDTH=1280
PREVIEW_MAX_HEIGHT=720

# Redis (optional - will fallback to sync processing if not available)
# REDIS_URL=redis://localhost:6379

# CORS
CORS_ORIGINS=http://localhost:5173,http://localhost:5174,http://localhost:5175,http://localhost:5176,http://localhost:5177,http://localhost:5178

# Security
ENABLE_PROCESSING=true
MOCK_MODE=false